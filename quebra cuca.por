programa {
  funcao inicio() {  
inteiro numero

  // Leitura do número do usuário
  escreva("Digite a dimensão do quadrado: ")
  leia(numero)

  // Impressão do quadrado
  para linha de 1 ate numero faca
    para coluna de 1 ate numero faca
      escreva("* ")
    fimpara
    escreva("\\n")
  fimpara
fimprograma}
}